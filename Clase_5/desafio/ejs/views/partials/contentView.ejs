<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Nombre</th>
            <th scope="col">Precio</th>
            <th scope="col">Imagen</th>
        </tr>
    </thead>
    <tbody>

        <% products.forEach(function(product) { %>
            <tr>

                <th scope="row">
                    <%=product.id%>
                </th>
                <td>
                    <%=product.title%>
                </td>
                <td>
                    <%=product.price%>
                </td>
                <td><img src=<%=product.thumbnail%>/></td>
            </tr>
            <% }); %>


    </tbody>
</table>